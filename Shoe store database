INSERT INTO inventory VALUES (14, "boat", 3, 20, 35);
INSERT INTO inventory values (15, "shoe polish", 5, 1, 5);

--display the database ordered by price.
SELECT * FROM inventory ORDER BY cost;

--display the items that are cost $10 or more.
SELECT * FROM inventory WHERE cost >= 10;


