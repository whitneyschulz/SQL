--Create a shoe store database

CREATE TABLE inventory (ID INTEGER PRIMARY KEY,item TEXT, quantity INTEGER, cost INTEGER, retail INTEGER);

INSERT INTO inventory VALUES (1, "tennis shoe", 3, 30, 50);
INSERT INTO inventory VALUES (2, "sandal", 45, 10, 20);
